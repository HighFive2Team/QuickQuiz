@model IEnumerable<QuizProject.Domain.Question>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.QuestionName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Questiontype)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Video)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Shart)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.QuestionName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Questiontype)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Video)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Shart)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Idquestion }) |
            @Html.ActionLink("Details", "Details", new { id=item.Idquestion }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Idquestion })
        </td>
    </tr>
}

</table>
